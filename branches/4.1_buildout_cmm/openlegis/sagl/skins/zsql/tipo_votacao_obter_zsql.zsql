<dtml-comment>
connection_id : dbcon_interlegis
arguments : tip_votacao
max_rows : 100
max_cache : 100
</dtml-comment>
SELECT 
   tip_votacao,
   tip_votacao as tip_votacao_sel,
   des_tipo_votacao

FROM 
   tipo_votacao

<dtml-sqlgroup where>
  <dtml-sqltest tip_votacao op="=" type="int" optional><dtml-and>
  ind_excluido = 0
</dtml-sqlgroup>

ORDER BY
      tip_votacao
