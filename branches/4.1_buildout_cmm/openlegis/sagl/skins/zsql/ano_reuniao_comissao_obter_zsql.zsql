<dtml-comment>
connection_id : dbcon_interlegis
arguments : ano_reuniao=""
max_rows : 1000
max_cache : 100
</dtml-comment>

SELECT DISTINCT
    DATE_FORMAT(reuniao_comissao.dat_inicio_reuniao, '%Y') as ano_reuniao

FROM
    reuniao_comissao

<dtml-sqlgroup where>
    reuniao_comissao.ind_excluido = 0
</dtml-sqlgroup>

ORDER BY YEAR(dat_inicio_reuniao) DESC
