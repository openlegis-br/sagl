<dtml-comment>
connection_id : dbcon_interlegis
arguments : cod_tramitacao dat_fim_prazo
max_rows : 1
max_cache : 100
</dtml-comment>
UPDATE
    tramitacao
SET
    dat_encaminha = NOW(),
    dat_fim_prazo = 
    <dtml-if expr="dat_fim_prazo==None">null
     <dtml-else><dtml-sqlvar dat_fim_prazo type="nb" optional>
    </dtml-if>

<dtml-sqlgroup where>
   <dtml-sqltest cod_tramitacao op="=" type="int">
</dtml-sqlgroup>
