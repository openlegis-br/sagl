<dtml-comment>
connection_id : dbcon_interlegis
arguments :
max_rows : 1000
max_cache : 100
</dtml-comment>

SELECT 
  DATE(dat_fim_prazo) as dat_fim_prazo,
  DATE_FORMAT(dat_fim_prazo, '%d/%m/%Y') as data_fim_prazo
FROM
  materia_legislativa
WHERE
  dat_fim_prazo > DATE_ADD(NOW(), INTERVAL +15 DAY)
  AND ind_tramitacao = 1
  AND ind_excluido = 0
GROUP BY
  dat_fim_prazo
ORDER BY 
  dat_fim_prazo
