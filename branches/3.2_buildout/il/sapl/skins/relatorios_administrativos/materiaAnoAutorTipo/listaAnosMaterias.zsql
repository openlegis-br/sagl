<dtml-comment>
connection_id : dbcon_interlegis
max_rows : 1000
max_cache : 100
</dtml-comment>
SELECT DISTINCT
     m.ano_ident_basica as ano_mat

FROM
     materia_legislativa m,
     autoria a

WHERE
     a.cod_materia = m.cod_materia
     and a.ind_excluido = 0
     and m.ind_excluido = 0

ORDER BY
     1 desc

