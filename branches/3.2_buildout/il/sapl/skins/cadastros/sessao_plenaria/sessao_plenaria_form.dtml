                  <dtml-call expr="REQUEST.set('temAtaODT', 1)">
                  <dtml-let documento_odt="_.getattr(sapl_documentos.ata_sessao,id_documento_odt).absolute_url">
                  <dtml-call expr="REQUEST.set('nome_arquivo_odt',_.str(cod_sessao_plen)+'_ata_sessao.odt')">
                  <a class="popup1" href="&dtml-portal_url;/generico/viewerJS/#<dtml-var documento_odt>?<dtml-var expr="ZopeTime().timeTime()">" data-fancybox-type="iframe">
                    <img align="top" border="0" src="&dtml-portal_url;/imagens/icon-odt.png" alt="Arquivo ODT" title="Visualizar ODT"></a>
                    <a class="botao-link" target="_blank" href="&dtml-URL1;/sapl_documentos/ata_sessao/externalEdit_/<dtml-var nome_arquivo_odt>.zem">Editor Externo</a>
                    <a class="botao-link" href="javascript:ata_odt_excluir()">Excluir</a>
                  </dtml-let>
               <dtml-else>
                  <a class="botao-link" href="javascript:ata_odt_gerar();">Gerar ODT</a>
               </dtml-if>
               </dtml-let>
          </td>
   </dtml-if>
           <td <dtml-if cod_sessao_plen>colspan="2"<dtml-else>colspan="4"</dtml-if>><label>Ata da Sessão (PDF)</label><br/>
            <input type="hidden" name="hdn_file_ata" value="0"/>
            <dtml-if cod_sessao_plen>
             <dtml-call expr="REQUEST.set('id_ata', _.str(cod_sessao_plen)+'_ata_sessao.pdf')">
             <dtml-if "_.hasattr(sapl_documentos.ata_sessao,id_ata)">
              <dtml-let ata="_.getattr(sapl_documentos.ata_sessao,id_ata).absolute_url">
               <a href="<dtml-var ata>?<dtml-var expr="ZopeTime().timeTime()">" target="_blank"><img align="middle" src="&dtml-portal_url;/imagens/archiv.gif" alt="Texto Integral"></a>
              </dtml-let>
               <input type="radio" name="radTI2" value="M" checked="true" onClick="ativa_desativa_file_field_ata(document.sessao_plenaria_form)"/><span class="texto">Manter</span>
               <input type="radio" name="radTI2" value="E" onClick="ativa_desativa_file_field_ata(document.sessao_plenaria_form)"/><span class="texto">Excluir</span>
               <input type="radio" name="radTI2" value="S" onClick="ativa_desativa_file_field_ata(document.sessao_plenaria_form)"/><span class="texto">Substituir</span>
               <input type="file" id="file_nom_arquivo_ata" name="file_nom_arquivo_ata" accept="application/pdf" size="40" maxlength="100" disabled="1" />
             <dtml-else>
               <input type="file" id="file_nom_arquivo_ata" name="file_nom_arquivo_ata" accept="application/pdf" size="40" maxlength="100" />
               <input type="hidden" name="radTI2" value="I"/>
               <dtml-if expr="_.has_key('temAtaODT')">
                 <a class="botao-link" href="javascript:ata_pdf_gerar()">Gerar PDF</a>
               </dtml-if>
             </dtml-if>
            <dtml-else>
              <input type="file" id="file_nom_arquivo_ata" name="file_nom_arquivo_ata" accept="application/pdf" size="40" maxlength="100" />
              <input type="hidden" name="radTI2" value="I"/>
               <dtml-if expr="_.has_key('temAtaODT')">
                 <a class="botao-link" href="javascript:ata_pdf_gerar()">Gerar PDF</a>
               </dtml-if>
            </dtml-if>
          </td>
        </tr>
            <tr>
                 <td colspan="2">
                    <label for="txt_url_audio">URL Arquivo Áudio <span class="texto">(Formatos MP3 / AAC)</span> </label> <br />
                    <input type="text" id="txt_url_audio" name="txt_url_audio" size="40"maxlength="150" value="<dtml-var url_audio missing null="">" /> <br />
               <dtml-if url_audio>
                 <input class="botao" type="button" value="Visualizar" onClick="javascript:window.open('sessao_plenaria_player_audio?cod_sessao_plen=<dtml-var cod_sessao_plen>&url_audio=<dtml-var url_audio>','PlayerAudio','width=540,height=435,top=200,left=200,scrollbars=no');"/>
                </dtml-if>
                </td>
                 <td colspan="2">
                    <label for="txt_url_video">URL Arquivo Vídeo <span class="texto">(Formatos MP4 / FLV / WebM)</span> </label> <br />
                    <input type="text" id="txt_url_video" name="txt_url_video" size="40" maxlength="150" value="<dtml-var url_video missing null="">" /> <br />
                <dtml-if url_video>
                 <input class="botao" type="button" value="Visualizar" onClick="javascript:window.open('sessao_plenaria_player_video?cod_sessao_plen=<dtml-var cod_sessao_plen>&url_video=<dtml-var url_video>','PlayerVideo','width=540,height=435,top=200,left=200,scrollbars=no');"/>
                </dtml-if>
                </td>         
            </tr>
          <dtml-if cod_sessao_plen>
            <tr>
              <td colspan="2">
                    <label for="txt_url_fotos">URL Álbum de Fotos <span class="texto"></span> </label> <br />
                    <input type="text" id="txt_url_fotos" name="txt_url_fotos" size="40" maxlength="150" value="<dtml-var url_fotos missing null="">" />
              </td>
              <td colspan="2"><label for="txt_texto_odt">Roteiro da Sessão (PDF)</label><br />
                 <dtml-call expr="REQUEST.set('id_roteiro', _.str(cod_sessao_plen)+'_roteiro_sessao.pdf')">
                 <dtml-if "_.hasattr(sapl_documentos.pauta_sessao,id_roteiro)">
                   <dtml-let roteiro="_.getattr(sapl_documentos.pauta_sessao,id_roteiro).absolute_url">
                     <a href="<dtml-var roteiro>?<dtml-var expr="ZopeTime().timeTime()">" target="_blank"><img align="middle" src="&dtml-portal_url;/imagens/archiv.gif" alt="Texto Integral"></a>
                     <input type="button" class="botao" value="Excluir" onclick="javascript:roteiro_excluir()">
                   </dtml-let>
                 <dtml-else>
                   <input type="file" id="file_nom_arquivo_roteiro" name="file_nom_arquivo_roteiro" accept="application/pdf" size="40" maxlength="100" />
                 </dtml-if>
              </td>
            </tr>
          <dtml-else>
              <input type="hidden" name="txt_url_fotos" value="" />
          </dtml-if>
</table>

<p align="center">
  <input type="hidden" name="dat_sessao" value="<dtml-var dat_inicio_sessao missing>" />
  <input class="botao" type="button" name="button" value="Salvar" onClick="campos_criticar()" />
  <input class="botao" type="button" value="Voltar" OnClick="history.go(-1)" />
</p>

</fieldset>
