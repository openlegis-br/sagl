                  </dtml-if>
             </dtml-if>
          </td>
         </tr>

          <td colspan="2"><label for="txt_texto_odt">Lista de Oradores (ODT)</label><br />
               <dtml-let id_documento_odt="_.str(cod_sessao_plen)+'_oradores_expediente.odt'">
               <dtml-if "_.hasattr(sapl_documentos.oradores_expediente,id_documento_odt)">
                  <dtml-let documento_odt="_.getattr(sapl_documentos.oradores_expediente,id_documento_odt).absolute_url">
                  <dtml-call expr="REQUEST.set('nome_arquivo_odt',_.str(cod_sessao_plen)+'_oradores_expediente.odt')">
                  <a class="popup1" href="&dtml-portal_url;/generico/viewerJS/#<dtml-var documento_odt>?<dtml-var expr="ZopeTime().timeTime()">" data-fancybox-type="iframe">
                    <img align="top" border="0" src="&dtml-portal_url;/imagens/icon-odt.png" alt="Arquivo ODT" title="Visualizar ODT"></a>
                    <a class="botao-link" target="_blank" href="&dtml-URL1;/sapl_documentos/oradores_expediente/externalEdit_/<dtml-var nome_arquivo_odt>.zem">Editor Externo</a>
                    <a class="botao-link" href="javascript:oradores_odt_excluir()">Excluir</a>
                  </dtml-let>
               <dtml-else>
                  <a class="botao-link" href="javascript:oradores_odt_gerar();">Gerar ODT</a>
               </dtml-if>
               </dtml-let>
          </td>
          <td colspan="2"><label>Lista de Oradores (PDF)</label><br/>
             <dtml-let documento_odt="_.str(cod_sessao_plen)+'_oradores_expediente.odt'">
              <dtml-if "_.hasattr(sapl_documentos.oradores_expediente,documento_odt)">
