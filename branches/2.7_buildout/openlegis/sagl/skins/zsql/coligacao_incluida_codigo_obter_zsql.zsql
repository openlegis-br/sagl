<dtml-comment>
connection_id : dbcon_openlegis
max_rows : 1000
max_cache : 100
</dtml-comment>
<dtml-if expr="pysc.verifica_conector_bd_pysc() == 1">

SELECT DISTINCT REPLACE(LAST_INSERT_ID(), 'L', '') as cod_coligacao FROM coligacao
  
<dtml-else>

SELECT DISTINCT MAX(cod_coligacao) FROM coligacao

</dtml-if>