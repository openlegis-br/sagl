<dtml-comment>
connection_id : dbcon_openlegis
max_rows : 1000
max_cache : 100
</dtml-comment>
<dtml-if expr="pysc.verifica_conector_bd_pysc() == 1">
SELECT
    DISTINCT REPLACE(LAST_INSERT_ID(), 'L', '') as cod_proposicao
FROM
    proposicao

<dtml-else>
SELECT
    DISTINCT MAX(cod_proposicao) as cod_proposicao
FROM
    proposicao
</dtml-if>