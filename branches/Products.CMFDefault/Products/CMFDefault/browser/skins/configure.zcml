<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">

  <browser:page
      for="*"
      name="ursine_globals"
      class=".ursa.UrsineGlobals"
      permission="zope.Public"
      />

  <browser:page
      for="*"
      layer="Products.CMFDefault.interfaces.ICMFDefaultSkin"
      name="icons.css"
      class=".icons.View"
      permission="zope.Public"
      />

</configure>
