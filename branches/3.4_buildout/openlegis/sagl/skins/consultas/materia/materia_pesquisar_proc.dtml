<dtml-var standard_html_header>

<script type="text/javascript">
$(document).ready(function() {
	$(".popup").fancybox({
		maxWidth	: 650,
		maxHeight	: 450,
		fitToView	: true,
		width		: '70%',
		height		: '75%',
		autoSize	: false,
		closeClick	: false,
                scrolling       : false,
                preload         : true,
		openEffect	: 'fade',
		closeEffect	: 'fade'
	});
});
</script>

<a id="ajuda" href="&dtml-portal_url;/ajuda/consultas#cons_materia">Ajuda</a>

<dtml-if hdn_cod_autor>
  <dtml-in expr="zsql.autor_obter_zsql(cod_autor=hdn_cod_autor)">
     <dtml-call expr="REQUEST.set('lst_tip_autor', des_tipo_autor)">
     <dtml-call expr="REQUEST.set('hdn_txt_autor', nom_autor_join)">
  </dtml-in>
<dtml-else>
     <dtml-call expr="REQUEST.set('lst_tip_autor', '')">
     <dtml-call expr="REQUEST.set('hdn_txt_autor', '')">
</dtml-if>

<dtml-if cod_relator>
    <dtml-call expr="REQUEST.set('cod_relator', cod_relator)">
<dtml-else>
    <dtml-call expr="REQUEST.set('cod_relator', '')">
</dtml-if>

<dtml-if lst_tip_materia>
    <dtml-call expr="REQUEST.set('lst_tip_materia', str(lst_tip_materia))">
<dtml-else>
    <dtml-call expr="REQUEST.set('lst_tip_materia', '')">
</dtml-if>

<dtml-let isAnon="portal_membership.isAnonymousUser()">
  <dtml-if expr="not isAnon">
    <a id="ajuda" href="<dtml-var portal_url>/relatorios/pdf_materia_preparar_pysc?<dtml-var "REQUEST.get('QUERY_STRING','')">&tipo_materia=<dtml-var "REQUEST.form.get('lst_tip_materia')">" target="_blank">PDF</a>
    <a id="ajuda" href="<dtml-var portal_url>/consultas/materia/exportar_materias_ods_pysc?<dtml-var "REQUEST.get('QUERY_STRING','')">&tipo_materia=<dtml-var "REQUEST.form.get('lst_tip_materia')">" target="_blank">ODS</a>
  </dtml-if>
</dtml-let>

<a id="rss" href="&dtml-portal_url;/generico/RSS2_materias?<dtml-var "REQUEST.get('QUERY_STRING','')">&tipo_materia=<dtml-var "REQUEST.form.get('lst_tip_materia')">" target="_blank">RSS</a>

<h1 class="firstHeading">Pesquisa de Matérias Legislativas</h1>

<dtml-if expr="REQUEST.get('g-recaptcha-response')!=''">

<dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Materia'])">
<div id="barra-conteudo">
    <ul>
        <li>
            <a href="&dtml-portal_url;/cadastros/materia/materia_form_resumido">Incluir Matéria</a>
        </li>
    </ul>
</div>
</dtml-if>

<dtml-call "REQUEST.set('existe_seq_anterior', 0)">
<dtml-call "REQUEST.set('existe_seq_posterior', 0)">
<dtml-if page>
  <dtml-call "REQUEST.set('page', _.int(page))">
<dtml-else>
  <dtml-call "REQUEST.set('page', 1)">
</dtml-if>
<dtml-if step>
  <dtml-call "REQUEST.set('step', _.int(step))">
<dtml-else>
  <dtml-call "REQUEST.set('step', 10)">
</dtml-if>
<dtml-call "REQUEST.set('start',(page-1) * step + 1)">

<dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Materia'])">
  <dtml-call expr="REQUEST.set('metodo', zsql.materia_pesquisar_zsql)">
<dtml-else>
  <dtml-call expr="REQUEST.set('metodo', zsql.materia_pesquisar_publico_zsql)">
</dtml-if>

<dtml-in expr="metodo(tip_id_basica=lst_tip_materia, num_ident_basica=pysc.extrai_id_pysc(id=txt_numero),
                      ano_ident_basica=txt_ano, ind_tramitacao=rad_tramitando, cod_relator=cod_relator,
                      des_assunto=txt_assunto, cod_status=lst_status,
                      cod_autor=hdn_cod_autor, num_protocolo=txt_num_protocolo,
                      dat_apresentacao=dt_apres, dat_apresentacao2=dt_apres2, 
                      dat_publicacao=dt_public, dat_publicacao2=dt_public2,
                      cod_unid_tramitacao=lst_localizacao, rd_ordem=rd_ordenacao)"
                      size="step" start="start" orphan=0>
<dtml-if sequence-start>
 <table>
  <tr>
   <th colspan="2" class="form-label">
    <b>Resultado da Pesquisa: <dtml-var sequence-length> matéria<dtml-if expr="_['sequence-length'] > 1">s</dtml-if> encontrada<dtml-if expr="_['sequence-length'] > 1">s</dtml-if>.
       <dtml-if expr="_['sequence-length'] == 5000"><br />LIMITE DE RESULTADOS ATINGIDO! Melhore os argumentos da pesquisa.</dtml-if>
   </th>
  </tr>
    <dtml-if previous-sequence>
      <dtml-call "REQUEST.set('existe_seq_anterior', 1)">
    </dtml-if previous-sequence>
   </dtml-if sequence-start>
            <tr <dtml-if sequence-odd>class="even"</dtml-if>>          
              <td width="83%" class="texto">
                <dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Materia'])">
                  <a href="&dtml-portal_url;/cadastros/materia/materia_mostrar_proc?cod_materia=<dtml-var cod_materia>">
                <dtml-else>
                  <a href="&dtml-portal_url;/consultas/materia/materia_mostrar_proc?cod_materia=<dtml-var expr="pysc.b64encode_pysc(codigo=cod_materia)">" >
                </dtml-if>
                    <span class="titulo">
                      <b><dtml-var des_tipo_materia> nº <dtml-var num_ident_basica>/<dtml-var ano_ident_basica></b>
                    </span>
                </a>
                 </br>
          <dtml-comment>verifica se usuário é anônimo</dtml-comment>
          <dtml-if expr="portal_membership.isAnonymousUser()">
                  <dtml-call expr="REQUEST.set('substituiEmentaTitulo', '0')">
                  <dtml-comment>verifica se matéria trata de título honorífico a partir de 2014</dtml-comment>
                  <dtml-if expr="sgl_tipo_materia=='PDL' and txt_indexacao=='TÍTULO' and ano_ident_basica>=2014">
                  <dtml-call expr="REQUEST.set('substituiEmentaTitulo', '1')">
                    <dtml-call expr="REQUEST.set('sgl_status', '')">
                    <dtml-in expr="zsql.tramitacao_obter_zsql(cod_materia=cod_materia)">
                     <dtml-if expr="cod_tramitacao and cod_tramitacao!=''">
                       <dtml-call expr="REQUEST.set('sgl_status', sgl_status)">
                       <dtml-comment>verifica se matéria foi aprovada ou promulgada pela Câmara</dtml-comment>
                       <dtml-if expr="sgl_status=='APROVADA' or sgl_status=='LEI CAMARA'">
                         <dtml-call expr="REQUEST.set('substituiEmentaTitulo', '0')">
                       </dtml-if>
                     </dtml-if>
                    </dtml-in>
                  </dtml-if>
                  <dtml-comment>mostra a ementa completa somente após aprovação ou promulgação</dtml-comment>
                  <dtml-if expr="substituiEmentaTitulo=='1'">
                    CONCEDE TÍTULO HONORÍFICO<br />
                  <dtml-else>
                     <dtml-var txt_ementa missing><br />
                  </dtml-if>
          <dtml-else>
                  <dtml-var txt_ementa missing><br />
          </dtml-if>

                <dtml-comment>obtém a autoria da matéria.</dtml-comment>
                <dtml-if expr="pysc.conta_autores_pysc(cod_materia)">
                  <dtml-if expr="pysc.conta_autores_pysc(cod_materia)>1">
                      <b>Autores:</b>
                  <dtml-else>
                      <b>Autoria:</b>
                  </dtml-if>
                </dtml-if>  
                <dtml-in expr="zsql.autoria_obter_zsql(cod_materia=cod_materia)">
                  <dtml-var nom_autor_join><dtml-unless sequence-end>,</dtml-unless>
                  <dtml-if sequence-end><br /></dtml-if>
                </dtml-in>

               <dtml-in expr="zsql.numeracao_obter_zsql(cod_materia=cod_materia)">
               <dtml-if num_materia>
               <b>Processo:</b>&nbsp;<dtml-var num_materia>/<dtml-var ano_materia> <br />
               </dtml-if>
               </dtml-in>

               <dtml-if num_protocolo>
                 <b>Protocolo Geral:</b>&nbsp;<dtml-var num_protocolo>/<dtml-var ano_ident_basica> <br />
               </dtml-if>

               <b>Data de apresentação:</b> <dtml-var "pysc.iso_to_port_pysc(dat_apresentacao)"><br />

                <dtml-call expr="REQUEST.set('temSubstitutivo', '0')">
                <dtml-in expr="zsql.substitutivo_obter_zsql(cod_materia=cod_materia)">
                  <dtml-if cod_substitutivo>
                    <dtml-call expr="REQUEST.set('temSubstitutivo', '1')">
                  </dtml-if>
                </dtml-in>

                <dtml-call expr="REQUEST.set('temEmenda', '0')">
                <dtml-in expr="zsql.emenda_obter_zsql(cod_materia=cod_materia)">
                  <dtml-if cod_emenda>
                    <dtml-call expr="REQUEST.set('temEmenda', '1')">
                  </dtml-if>
                </dtml-in>

                <dtml-call expr="REQUEST.set('temDoc', '0')">
                <dtml-in expr="zsql.documento_acessorio_obter_zsql(cod_materia=cod_materia,ind_excluido=0)">
                  <dtml-if cod_documento>
                    <dtml-call expr="REQUEST.set('temDoc', '1')">
                  </dtml-if>
                </dtml-in>

                <dtml-if expr="temDoc=='1'">
                 <dtml-in expr="zsql.documento_acessorio_obter_zsql(cod_materia=cod_materia,ind_excluido=0)">
                   <dtml-if sequence-start>            
                      <b>Documento<dtml-if expr="_['sequence-length'] > 1">s</dtml-if> Acessório<dtml-if expr="_['sequence-length'] > 1">s</dtml-if>:</b>
                   </dtml-if>
                   <dtml-in expr="zsql.proposicao_obter_zsql(ind_mat_ou_doc='D',cod_mat_ou_doc=cod_documento)">
                    <dtml-let id_proposicao="_.str(cod_proposicao)+'_signed.pdf'">
                      <dtml-if "_.hasattr(sagl_documentos.proposicao,id_proposicao)">
                        <dtml-let proposicao="_.getattr(sagl_documentos.proposicao,id_proposicao).absolute_url">
                           <a target="blank" href="<dtml-var proposicao>?<dtml-var expr="ZopeTime().timeTime()">">
                             <dtml-var nom_documento>
                           </a>
                        </dtml-let>
                      </dtml-if>
                    </dtml-let>
                   <dtml-else>
                    <dtml-call expr="REQUEST.set('id_documento', _.str(cod_documento)+'.pdf')">
                    <dtml-if expr="_.hasattr(sagl_documentos.materia,id_documento)">
                      <dtml-let documento="_.getattr(sagl_documentos.materia,id_documento).absolute_url">
                        <a href="<dtml-var documento>?<dtml-var expr="ZopeTime().timeTime()">" target="_blank">
                          <dtml-var nom_documento>
                        </a>
                      </dtml-let>
                    <dtml-else>
                        <dtml-var nom_documento>
                    </dtml-if>
                   </dtml-in>
                   <dtml-unless sequence-end>,</dtml-unless><dtml-if sequence-end><br /></dtml-if>
                 </dtml-in>
                </dtml-if>

                <dtml-if expr="temSubstitutivo=='1' or temEmenda=='1'">
                 <b>Matérias Acessórias:</b> 
                  <dtml-in expr="zsql.substitutivo_obter_zsql(cod_materia=cod_materia)">
                    <dtml-if sequence-start>
                      <dtml-var sequence-length> Substitutivo<dtml-if expr="_['sequence-length'] > 1">s</dtml-if>
                    </dtml-if sequence-start>
                  </dtml-in>
                  <dtml-in expr="zsql.emenda_obter_zsql(cod_materia=cod_materia)">
                    <dtml-if sequence-start>
                      <dtml-var sequence-length> Emenda<dtml-if expr="_['sequence-length'] > 1">s</dtml-if>
                    </dtml-if sequence-start>
                  </dtml-in><br />
                </dtml-if> 

<dtml-comment> 
               <dtml-in expr="zsql.votacao_materia_data_obter_zsql(cod_materia=cod_materia)">
                <dtml-if cod_sessao_plen>
                  <dtml-in expr="zsql.sessao_plenaria_obter_zsql(cod_sessao_plen=cod_sessao_plen,ind_excluido=0)">
                    <b>Deliberação em Plenário:</b>
                    <dtml-in expr="zsql.tipo_sessao_plenaria_obter_zsql(tip_sessao=tip_sessao)">
                      <dtml-if expr="incluir=='1'">
                         <a href="&dtml-portal_url;/cadastros/sessao_plenaria/sessao_plenaria_mostrar_proc?cod_sessao_plen=<dtml-var cod_sessao_plen>&dat_sessao=<dtml-var dat_sessao>">
                      <dtml-else>
                         <a href="&dtml-portal_url;/consultas/sessao_plenaria/agenda_sessao_plen_mostrar_proc?cod_sessao_plen=<dtml-var cod_sessao_plen>&dat_sessao=<dtml-var dat_sessao>">
                      </dtml-if>
                         <dtml-var num_sessao_plen>ª <dtml-if expr="nom_sessao!='Audiência Pública'">Sessão</dtml-if> <dtml-var nom_sessao> - <dtml-var dat_sessao>
                         </a><br />
                    </dtml-in>
                  </dtml-in>
                </dtml-if>
               </dtml-in>
</dtml-comment>
                <dtml-call expr="REQUEST.set('des_status', '')">
                <dtml-call expr="REQUEST.set('txt_tramitacao', '')">
                <dtml-comment>obtém a última tramitação da matéria.</dtml-comment>
                <dtml-in expr="zsql.tramitacao_obter_zsql(cod_materia=cod_materia, ind_ult_tramitacao=1)">
                  <dtml-if cod_unid_tram_dest>
                    <dtml-call expr="REQUEST.set('cod_unid_tram', cod_unid_tram_dest)">
                  <dtml-else>
                    <dtml-call expr="REQUEST.set('cod_unid_tram', cod_unid_tram_local)">
                  </dtml-if>
                  <dtml-comment>obtém a unidade de tramitação da matéria.</dtml-comment>
                  <dtml-in expr="zsql.unidade_tramitacao_obter_zsql(cod_unid_tramitacao=cod_unid_tram)">
                    <dtml-if cod_orgao>
                      <b>Localização Atual: </b><dtml-var nom_orgao><br />
                    <dtml-elif cod_comissao>
                      <b>Localização Atual: </b><dtml-var nom_comissao><br />
                    <dtml-else> 
                      <b>Localização Atual: </b><dtml-var nom_parlamentar><br />
                    </dtml-if>
                  </dtml-in>
                  <dtml-call expr="REQUEST.set('des_status', des_status)">
                  <dtml-call expr="REQUEST.set('txt_tramitacao', txt_tramitacao)">
                <dtml-if des_status>
                  <b>Situação:</b> <dtml-var des_status null="">
                  <dtml-if dat_fim_prazo>
                    | <b>Prazo: </b> <dtml-var dat_fim_prazo>
                  </dtml-if><br />
                  <dtml-if expr="txt_tramitacao!=None and txt_tramitacao!=''">
                    <b>Texto da ação: </b> <dtml-var txt_tramitacao><br />
                  </dtml-if>
                </dtml-if>
               </dtml-in>

                 <dtml-in expr="zsql.anexada_obter_zsql(cod_materia_anexada=cod_materia, ind_excluido=0)">
                  <dtml-if sequence-start>
                    <b>Matéria Anexadora:</b>
                  </dtml-if>
                  <dtml-in expr="zsql.materia_obter_zsql(cod_materia=cod_materia_principal, ind_excluido = 0)">
                    <dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Materia'])">
                      <a href="&dtml-portal_url;/cadastros/materia/materia_mostrar_proc?cod_materia=<dtml-var cod_materia>">
                    <dtml-else>
                      <a href="&dtml-portal_url;/consultas/materia/materia_mostrar_proc?cod_materia=<dtml-var expr="pysc.b64encode_pysc(codigo=cod_materia)">" >
                    </dtml-if>
                       <b><dtml-var des_tipo_materia> nº <dtml-var num_ident_basica missing null="">/<dtml-var ano_ident_basica missing null=""></b></a>
                  </dtml-in><dtml-unless sequence-end>,</dtml-unless><dtml-if sequence-end><br /></dtml-if>
                 </dtml-in>

                <dtml-in expr="zsql.anexada_obter_zsql(cod_materia_principal=cod_materia, ind_excluido=0)">
                  <dtml-if sequence-start>
                    <b>Matéria Anexada:</b>
                  </dtml-if>
                   <dtml-in expr="zsql.materia_obter_zsql(cod_materia=cod_materia_anexada, ind_excluido = 0)">
                      <dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Materia'])">
                        <a href="&dtml-portal_url;/cadastros/materia/materia_mostrar_proc?cod_materia=<dtml-var cod_materia>">
                      <dtml-else>
                        <a href="&dtml-portal_url;/consultas/materia/materia_mostrar_proc?cod_materia=<dtml-var expr="pysc.b64encode_pysc(codigo=cod_materia)">" >
                      </dtml-if>
                       <b><dtml-var des_tipo_materia> nº <dtml-var num_ident_basica missing null="">/<dtml-var ano_ident_basica missing null=""></b></a>
                   </dtml-in><dtml-unless sequence-end>,</dtml-unless><dtml-if sequence-end><br /></dtml-if>
               </dtml-in>

                <dtml-comment>informa se possui cod_situacao e não possui tramitação </dtml-comment>
                <dtml-in expr="zsql.tramitacao_obter_zsql(cod_materia=cod_materia)">
                 <dtml-if expr="cod_tramitacao and cod_tramitacao!=''">
                    <dtml-call "REQUEST.set('com_tramitacao', 1)">
                 </dtml-if>
                </dtml-in>
         	<dtml-if txt_observacao>
	        <dtml-elif expr="cod_situacao and not _.has_key('com_tramitacao')">
                   <dtml-in expr="zsql.tipo_situacao_materia_obter_zsql(tip_situacao_materia=cod_situacao)">
                       <b>Situação:</b> <dtml-var des_tipo_situacao missing><br />
                   </dtml-in>
                </dtml-if>

                <dtml-comment>obtém a norma jurídica derivada </dtml-comment>
                <dtml-in expr="zsql.materia_buscar_norma_juridica_zsql(cod_materia=cod_materia)">
                   <dtml-if expr="cod_norma and cod_norma!=''">
                     <b>Norma Derivada:</b>
                     <dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Norma'])">
                        <a href="&dtml-portal_url;/cadastros/norma_juridica/norma_juridica_mostrar_proc?cod_norma=<dtml-var cod_norma>"><b><dtml-var des_norma> nº <dtml-var num_norma>/<dtml-var ano_norma></b></a><br />
                     <dtml-else>
                        <a href="&dtml-portal_url;/consultas/norma_juridica/norma_juridica_mostrar_proc?cod_norma=<dtml-var cod_norma>"><b><dtml-var des_norma> nº <dtml-var num_norma>/<dtml-var ano_norma></b></a><br />
                     </dtml-if>
                   </dtml-if>
                </dtml-in>
                <dtml-if expr="int(sagl_documentos.props_sagl.acompanhamento_materia)==1">
                  <dtml-if expr="ind_tramitacao==1">
                  <img align="absmiddle" border="0" src="&dtml-portal_url;/imagens/acomp_email.png">&nbsp;<span style="color:#32682F;font-weight:normal;"><a href="&dtml-portal_url;/consultas/materia/acompanhamento/acomp_materia_form?cod_materia=<dtml-var cod_materia>" class="popup" data-fancybox-type="iframe"><b>Acompanhar matéria</b></a>&nbsp;</span>
                  </dtml-if>
                </dtml-if>
              </td>
              <td width="17%">
               <div align="center">
                <dtml-call expr="REQUEST.set('redacao_final', _.str(cod_materia)+'_redacao_final.pdf')">
                <dtml-call expr="REQUEST.set('texto_original', _.str(cod_materia)+'_texto_integral.pdf')">
                <dtml-if "_.hasattr(sagl_documentos.materia,redacao_final)">
                   <a target="_blank" href="&dtml-portal_url;/pysc/download_materia_pysc?cod_materia=<dtml-var expr="pysc.b64encode_pysc(codigo=cod_materia)">&redacao_final=1" alt="Redação Final" title="Redação Final">
                      <img align="middle" src="&dtml-portal_url;/imagens/archiv.gif" alt="Redação Final" title="Redação Final" />
                      <p style="line-height: 1.3em; font-size:11px"><b>Redação Final</b></p>
                   </a>
                <dtml-elif "_.hasattr(sagl_documentos.materia,texto_original)">
                   <a target="_blank" href="&dtml-portal_url;/pysc/download_materia_pysc?cod_materia=<dtml-var expr="pysc.b64encode_pysc(codigo=cod_materia)">&texto_original=1" alt="Texto Integral" title="Texto Integral">
                      <img align="middle" src="&dtml-portal_url;/imagens/archiv.gif" alt="Texto Integral" title="Texto Integral" />
                      <p style="line-height: 1.3em; font-size:11px"><b>Texto Integral</b></p>
                   </a>
                <dtml-else>
                  <img class="img-disabled" src="&dtml-portal_url;/imagens/archiv.gif" alt="Arquivo não disponível" title="Arquivo não disponível">
                </dtml-if>
               </div>
              </td>
            </tr>
  <dtml-if sequence-end>
 </table>
    <dtml-if next-sequence>
      <dtml-call "REQUEST.set('existe_seq_posterior', 1)">

    </dtml-if next-sequence>
    <dtml-if expr="existe_seq_anterior or existe_seq_posterior">
     <p align="center" class="texto">
        <b><dtml-var "pysc.PageListOutput_pysc('materia_pesquisar_proc', page, step, _['sequence-length'], incluir=incluir, txt_num_protocolo=txt_num_protocolo,cod_relator=cod_relator, existe_ocorrencia=existe_ocorrencia,lst_tip_materia=lst_tip_materia,txt_numero=txt_numero,txt_ano=txt_ano,dt_apres=dt_apres,dt_apres2=dt_apres2,dt_public=dt_public,dt_public2=dt_public2,hdn_cod_autor=hdn_cod_autor,txt_assunto=txt_assunto,rad_tramitando=rad_tramitando,lst_status=lst_status,lst_localizacao=lst_localizacao,lst_tip_autor=lst_tip_autor,hdn_txt_autor=hdn_txt_autor,rd_ordenacao=rd_ordenacao)">
      </b></p>
    </dtml-if>
</dtml-if sequence-end>
<dtml-else>
