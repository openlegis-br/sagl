select  DATE_FORMAT(MIN(dat_tramitacao), '%d/%m/%Y') as dat_inicio, 
        DATE_FORMAT(MAX(dat_tramitacao), '%d/%m/%Y') as dat_fim
 from  tramitacao     
where  ind_excluido="0"
  

