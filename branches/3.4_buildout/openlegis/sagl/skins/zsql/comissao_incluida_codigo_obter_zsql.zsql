<dtml-comment>
connection_id : dbcon_interlegis
max_rows : 1000
max_cache : 100
</dtml-comment>


<dtml-if expr="pysc.verifica_conector_bd_pysc() == 1">

   SELECT distinct REPLACE(LAST_INSERT_ID(), 'L', '') as cod_comissao FROM comissao

<dtml-else>

   SELECT MAX(cod_comissao) as cod_comissao FROM comissao

</dtml-if>
