<dtml-comment>
connection_id : dbcon_interlegis
arguments : ano_sessao=""
max_rows : 1000
max_cache : 100
</dtml-comment>

SELECT DISTINCT
    DATE_FORMAT(sessao_plenaria.dat_inicio_sessao, '%Y') as ano_sessao

FROM
    sessao_plenaria

<dtml-sqlgroup where>
    sessao_plenaria.ind_excluido = 0
</dtml-sqlgroup>

ORDER BY YEAR(dat_inicio_sessao) DESC
