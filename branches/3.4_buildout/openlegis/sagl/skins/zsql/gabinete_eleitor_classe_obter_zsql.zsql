<dtml-comment>
connection_id : dbcon_interlegis
arguments : cod_parlamentar
max_rows : 100
max_cache : 100
</dtml-comment>
SELECT 
  txt_classe 
FROM 
  gabinete_eleitor
WHERE 
  cod_parlamentar = <dtml-sqlvar cod_parlamentar type="int"> 
  AND (txt_classe IS NOT NULL or txt_classe != '') 
  AND ind_excluido =0
GROUP BY 
  txt_classe

