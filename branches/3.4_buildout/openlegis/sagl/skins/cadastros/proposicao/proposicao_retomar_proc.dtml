<dtml-try>
   <dtml-call expr="zsql.proposicao_retomar_zsql(cod_proposicao=cod_proposicao)">
<dtml-except>
  <dtml-let mensagem="'Houve um erro! Proposição não pode ser retomada!'">
    <dtml-var mensagem_emitir>
  </dtml-let>  
<dtml-else>
  <dtml-let mensagem="'Proposição retomada com sucesso!'" url="'proposicao_mostrar_proc?cod_proposicao='+cod_proposicao">
    <dtml-var mensagem_emitir>
  </dtml-let>
</dtml-try>
