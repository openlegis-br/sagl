<dtml-try>
 <dtml-call expr="zsql.pessoa_excluir_zsql(cod_pessoa = cod_pessoa)">
<dtml-except>
   <dtml-let mensagem="'Houve um erro! Pessoa não excluída!'">
      <dtml-var mensagem_emitir>
   </dtml-let>
<dtml-else>
   <dtml-let mensagem="'Pessoa excluída com sucesso!'" url="url">
      <dtml-var mensagem_emitir>
   </dtml-let>
</dtml-try>
