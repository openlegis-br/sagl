<dtml-try>
 <dtml-call expr="zsql.visita_excluir_zsql(cod_visita = cod_visita)">
<dtml-except>
   <dtml-let mensagem="'Houve um erro! Visita não excluída!'">
      <dtml-var mensagem_emitir>
   </dtml-let>
<dtml-else>
   <dtml-let mensagem="'Visita excluída com sucesso!'" url="'javascript:parent.jQuery.fancybox.close();'">
      <dtml-var mensagem_emitir>
   </dtml-let>
</dtml-try>
