<h1 class="firstHeading">Tramitação Eletrônica de Documento</h1>

  <table class="table" >
    <tr>
       <td>
          <dtml-in expr="zsql.documento_administrativo_obter_zsql(cod_documento=hdn_cod_documento)"><div align="center">
             <h2>
                <dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Modulo Administrativo'])">
                  <a href="&dtml-portal_url;/cadastros/documento_administrativo/documento_administrativo_mostrar_proc?cod_documento=<dtml-var cod_documento>" target="_blank">
                <dtml-else>
                  <a href="&dtml-portal_url;/consultas/documento_administrativo/documento_administrativo_mostrar_proc?cod_documento=<dtml-var cod_documento>" target="_blank">
                </dtml-if>
                <dtml-var des_tipo_documento> nº <dtml-var num_documento>/<dtml-var ano_documento> - 
                <dtml-var txt_interessado></a></h2>
             <dtml-var txt_assunto>
          </dtml-in></div>
       </td>
    </tr>
  </table>

<form name="tramitacao_form" method="post" action="tramitacao_salvar_proc" enctype="multipart/form-data">
  <input class="form-control" type="hidden" name="cod_usuario_corrente" value="<dtml-var cod_usuario_corrente missing>" />
  <input class="form-control" type="hidden" name="hdn_cod_tramitacao" value="<dtml-var hdn_cod_tramitacao missing>" />
  <input class="form-control" type="hidden" name="hdn_cod_documento" value="<dtml-var hdn_cod_documento missing>" />
  <input class="form-control" type="hidden" name="hdn_dat_tramitacao" value="<dtml-var dat_tramitacao missing>" />
  <input class="form-control" type="hidden" name="hdn_file" value="0"/>

           <dtml-if hdn_cod_tramitacao>
             <dtml-call expr="REQUEST.SESSION.set('hdn_cod_tramitacao', hdn_cod_tramitacao)">
             <dtml-in expr="zsql.tramitacao_administrativo_obter_zsql(cod_tramitacao=hdn_cod_tramitacao)">
                  <dtml-call expr="REQUEST.set('hdn_cod_unid_tram_local', int(cod_unid_tram_local))">
                  <dtml-call expr="REQUEST.set('hdn_cod_unid_tram_dest', int(cod_unid_tram_dest))">
                  <dtml-call expr="REQUEST.set('hdn_cod_status', int(cod_status))">
                  <dtml-call expr="REQUEST.set('hdn_txt_tramitacao', txt_tramitacao)">
                  <dtml-call expr="REQUEST.set('dat_encaminha', dat_encaminha)">
                  <dtml-call expr="REQUEST.set('dat_fim_prazo', dat_fim_prazo)">
              <dtml-if expr="hdn_cod_tramitacao and cod_usuario_dest">
                <dtml-call expr="REQUEST.set('cod_usuario_dest', int(cod_usuario_dest))">
              <dtml-else>
                <dtml-call expr="REQUEST.set('cod_usuario_dest', '')">
              </dtml-if>
             </dtml-in>
           </dtml-if>

 <table class="table" >
      <tr>
             <dtml-unless dat_tramitacao>
               <dtml-call expr="REQUEST.set('dat_tramitacao', _.DateTime().strftime('%d/%m/%Y'))">
             </dtml-unless>
            <dtml-if cod_tramitacao>             
              <dtml-if expr="pysc.browser_verificar_pysc(HTTP_USER_AGENT)"> 
                <input class="form-control" type="text" name="txt_dat_tramitacao" id="txt_dat_tramitacao" size="10" maxlength="10" value="<dtml-var dat_tramitacao missing>"
                  disabled />
              <dtml-else>
                <span class="form-optional"><dtml-var dat_tramitacao missing></span>
                  <td><input class="form-control" type="hidden" name="txt_dat_tramitacao" id="txt_dat_tramitacao" size="10" maxlength="10" value="<dtml-var dat_tramitacao missing>" />
              </dtml-if>
            <dtml-else>
              <input class="form-control" type="hidden" name="txt_dat_tramitacao" id="txt_dat_tramitacao" value="<dtml-var dat_tramitacao missing>" />
            </dtml-if>

        <td  width="50%">Unidade de Origem</br>
         <dtml-in expr="zsql.unidade_tramitacao_obter_zsql(cod_unid_tramitacao=hdn_cod_unid_tram_local,ind_adm=1)">
           <dtml-if cod_orgao><input class="form-control" type="text" size="50" value="<dtml-var nom_orgao>" readonly>
           <dtml-elif cod_comissao><input class="form-control" type="text" size="50" value="<dtml-var nom_comissao>" readonly>
           <dtml-else><input class="form-control" type="text" size="50" value="<dtml-var nom_parlamentar>" readonly>
           </dtml-if>
             <input class="form-control" type="hidden" name="hdn_cod_unid_tram_local" value="<dtml-var cod_unid_tramitacao>">
         </dtml-in>
        </td>
        <td width="25%">Usuário de Origem</br>
           <dtml-in expr="zsql.usuario_obter_zsql(col_username=AUTHENTICATED_USER.getUserName())">
             <input class="form-control" type="text" size="12" value="<dtml-var col_username>" readonly>
             <dtml-call expr="REQUEST.set('cod_usuario_local', cod_usuario)">
             <input class="form-control" type="hidden" name="hdn_cod_usuario_local" id="hdn_cod_usuario_local" value="<dtml-var cod_usuario>">
           </dtml-in>
       </td>
       <td width="25%">Encaminhamento</br>
         <dtml-if dat_encaminha>
           <input class="form-control" type="text" name="txt_dat_encaminha" size="13" value="<dtml-var dat_encaminha missing null>" readonly>
         <dtml-else>
           <input class="form-control" type="text" name="txt_dat_encaminha" size="13" value="" readonly>
         </dtml-if>
       </td>
      </tr>
      <tr>
       <dtml-in expr="zsql.unidade_tramitacao_obter_zsql(cod_unid_tramitacao=hdn_cod_unid_tram_local)">
           <dtml-call expr="REQUEST.set('unid_dest_permitidas',unid_dest_permitidas_sel)">
           <dtml-call expr="REQUEST.set('status_permitidos',status_adm_permitidos_sel)">
       </dtml-in>
        <td >Unidade de Destino </label><span class="campoObrigatorio">&nbsp;</span></br>
          <style scoped>div.dhxcombo_dhx_skyblue{margin-left: 0;}</style>
          <select class="custom-select"  name="lst_cod_unid_tram_dest" id="lst_cod_unid_tram_dest" style="max-width: 338px; width: 100%;">
            <dtml-in expr="zsql.unidade_tramitacao_obter_zsql(ind_adm=1)">
              <dtml-if expr="not dat_extincao and _.str(cod_unid_tramitacao) in _.string.split(_.str(unid_dest_permitidas),',')">
                <option 
                  <dtml-if expr="_.has_key('hdn_cod_unid_tram_dest') and hdn_cod_unid_tram_dest==int(cod_unid_tramitacao)"> selected
                  </dtml-if>
                  value="<dtml-var cod_unid_tramitacao>">
                <dtml-if cod_orgao><dtml-var nom_orgao>
                <dtml-elif cod_comissao><dtml-var nom_comissao> 
                <dtml-else><dtml-var nom_parlamentar> 
                </dtml-if></option>
               </dtml-if>
            </dtml-in>
          </select>
        </td>
        <style scoped>div.dhxcombo_dhx_skyblue{margin-left: 0;}</style>
        <td>Usuário de Destino</br>
             <select class="custom-select"  name="lst_cod_usuario_dest" id="lst_cod_usuario_dest" style="max-width: 110.25px; width: 100%;">
               <option value="<dtml-var cod_usuario_dest missing>">
                <dtml-if cod_usuario_dest>
                 <dtml-in expr="zsql.usuario_obter_zsql(cod_usuario=cod_usuario_dest)">
                   <dtml-var col_username missing>
                 </dtml-in>
                </dtml-if>
               </option>
             </select>
		<script type="text/javascript">
			myCombo = dhtmlXComboFromSelect("lst_cod_unid_tram_dest");
			myCombo.enableFilteringMode(true);
			myCombo2 = dhtmlXComboFromSelect("lst_cod_usuario_dest")
			myCombo2.enableFilteringMode(true);
			myCombo2.disable()
			myCombo.attachEvent("onChange", function(value){
    			myCombo2.clearAll();
    			myCombo2.setComboValue(null);
    			myCombo2.setComboText("");
    			if (value == null) {
       			 myCombo2.disable();
    			} else {
        			myCombo2.enable();
        			myCombo2.load("usuarios_carregar_pysc?svalue="+value);
    			}
			});
		</script>
        </td>
        <td>Recebimento</br>
         <dtml-if dat_recebimento>
           <input class="form-control" type="text" name="txt_dat_recebimento" size="13" value="<dtml-var dat_recebimento missing null>" readonly>
         <dtml-else>
           <input class="form-control" type="text" name="txt_dat_recebimento" size="13" value="" readonly>
         </dtml-if>
        </td>
      </tr>
      <tr>
        <td>
          <label for="lst_cod_status">Status </label><span class="campoObrigatorio">&nbsp;</span><br/>
          <select class="custom-select"  name="lst_cod_status" style="max-width: 338px; width: 100%;">
            <option></option>
            <dtml-in expr="zsql.status_tramitacao_administrativo_obter_zsql()">
              <dtml-if expr="_.str(cod_status) in _.string.split(_.str(status_permitidos),',')">
                <option 
                   <dtml-if expr="_.has_key('hdn_cod_status') and int(hdn_cod_status)==int(cod_status)"> selected
                   </dtml-if>
                  value="<dtml-var cod_status>">
                <dtml-var sgl_status> - <dtml-var des_status></option>
              </dtml-if>
            </dtml-in>
          </select>
        </td>
        <td colspan="2">
          <label for="txt_dat_fim_prazo">Data Fim Prazo </label><br />
          <input class="form-control" type="text" name="txt_dat_fim_prazo" id="txt_dat_fim_prazo" size="10" maxlength="10" value="<dtml-var dat_fim_prazo missing null="">" />
        </td>
      </tr>

  <dtml-if hdn_cod_tramitacao>
      <dtml-call expr="REQUEST.set('cod_tramitacao', hdn_cod_tramitacao)">
      <dtml-call expr="REQUEST.set('pdf', _.str(cod_tramitacao)+'_tram.pdf')">
      <dtml-call expr="REQUEST.set('pdf_anexo', _.str(cod_tramitacao)+'_tram_anexo1.pdf')">
      <dtml-call expr="REQUEST.set('pdf_assinado', _.str(cod_tramitacao)+'_tram_signed.pdf')">
      <dtml-if "_.hasattr(sapl_documentos.administrativo.tramitacao,pdf) or _.hasattr(sapl_documentos.administrativo.tramitacao,pdf_assinado)">
       <dtml-if "_.hasattr(sapl_documentos.administrativo.tramitacao,pdf)">
         <tr>
           <th>
             Tramitação em PDF
           </th>
           <th valign="top" colspan="2" style="text-align: left;">
                 <dtml-if "_.hasattr(sapl_documentos.administrativo.tramitacao,pdf)">
                   <dtml-let pdf="_.getattr(sapl_documentos.administrativo.tramitacao,pdf).absolute_url">
                        <a class="popup1" href="<dtml-var pdf>?<dtml-var expr="ZopeTime().timeTime()">" data-fancybox-type="iframe">
                       <img align="top" style="float: left; margin-right: 8px;" src="&dtml-portal_url;/imagens/archiv.gif" alt="Arquivo PDF" title="Visualizar PDF">
                   </dtml-let>
                 </dtml-if>
           </th>
         </tr>
         <tr>
           <td colspan="3">
                      <input class="form-control" type="radio" name="radTI" value="G" onClick="ativa_desativa_file_field(document.tramitacao_form)"/><span class="texto"><b>Gerar PDF</b></span>
                      <input class="form-control" type="radio" name="radTI" value="S" onClick="ativa_desativa_file_field(document.tramitacao_form)"/><span class="texto"><b>Carregar Arquivo</b></span>
                      <input class="form-control" type="file" id="file_nom_arquivo" name="file_nom_arquivo" accept="application/pdf" size="45" maxlength="100" disabled="1" />
                      <input class="form-control" type="radio" name="radTI" value="M" checked="true" onClick="ativa_desativa_file_field(document.tramitacao_form)"/><span class="texto"><b>Manter</b></span>
           </td>
         </tr>
       </dtml-if>
       <dtml-if "_.hasattr(sapl_documentos.administrativo.tramitacao,pdf_assinado)">
         <tr>
           <th>
                Tramitação em PDF
           </th>
           <th colspan="2">
                   <dtml-let pdf_assinado="_.getattr(sapl_documentos.administrativo.tramitacao,pdf_assinado).absolute_url">
                     <a class="popup1" href="<dtml-var pdf_assinado>?<dtml-var expr="ZopeTime().timeTime()">" data-fancybox-type="iframe">
                       <img height="22" style="float: left; margin-right: 8px;" src="&dtml-portal_url;/imagens/pdf_signed.gif" alt="PDF - Tramitação assinada" title="Visualizar PDF - Tramitação assinada">Tramitação Assinada
                     </a>
                   </dtml-let>
           </th>
         </tr>
         <tr>
           <td colspan="3">
              <input class="form-control" type="radio" name="radTI" value="M" checked="true" /><span class="texto"><b>Manter</b></span>
              <input class="form-control" type="radio" name="radTI" value="E" /><span class="texto"><b>Excluir</b></span>
              <input class="form-control" type="hidden" name="file_nom_arquivo" value=""/>
           </td>
         </tr>
       </dtml-if>

      <dtml-else>
                 <tr>
                   <td colspan="3">
                      <input class="form-control" type="radio" name="radTI" value="G" checked="true" onClick="ativa_desativa_file_field(document.tramitacao_form)"/><span class="texto"><b>Gerar PDF</b></span>
                      <input class="form-control" type="radio" name="radTI" value="U" onClick="ativa_desativa_file_field(document.tramitacao_form)"/><span class="texto"><b>Carregar Arquivo</b></span>
                      <input class="form-control" type="file" id="file_nom_arquivo" name="file_nom_arquivo" accept="application/pdf" size="45" maxlength="100" />
                   </td>
                 </tr>
      </dtml-if>
   <dtml-else>
         <tr>
           <td valign="top" colspan="3">
                      <input class="form-control" type="radio" name="radTI" value="G" checked="true" onClick="ativa_desativa_file_field(document.tramitacao_form)"/><span class="texto"><b>Gerar PDF</b></span>
              <input class="form-control" type="radio" name="radTI" value="U" onClick="ativa_desativa_file_field(document.tramitacao_form)"/><span class="texto"><b>Carregar Arquivo</b></span>
              <input class="form-control" type="file" id="file_nom_arquivo" name="file_nom_arquivo" accept="application/pdf" size="45" maxlength="100" disabled="1" />
           </td>
         </tr>
   </dtml-if>

      <tr>
        <td colspan="3">
          <label for="txa_txt_tramitacao">Texto do Despacho</label><br/>
          <textarea class="form-control"  name="txa_txt_tramitacao" id="txa_txt_tramitacao" cols="65" rows="4"><dtml-var hdn_txt_tramitacao missing null=""></textarea>
        </td>
      </tr>
 </table>
</form>

  <dtml-if dat_encaminha>
      <p align="center">
        <input class="form-control" class="botao" type="button" value="Voltar" onClick="location.href='rascunhos_html'">
      </p>
  <dtml-else>
     <form name="encaminhamento_registrar" method="post" action="encaminhamento_registrar_proc">
      <dtml-in expr="zsql.usuario_obter_zsql(col_username=AUTHENTICATED_USER.getUserName())">
        <input class="form-control" type="hidden" name="cod_usuario_corrente" value="<dtml-var cod_usuario missing>" />
      </dtml-in>
      <p align="center">
        <input class="form-control" type="hidden" name="cod_tramitacao" value="<dtml-var hdn_cod_tramitacao missing>" />
        <input class="btn btn-primary" type="button" class="botao" name="btn_tramitacao_salvar" value="Salvar" onClick="campos_criticar(document.tramitacao_form)" />
        <dtml-if hdn_cod_tramitacao>
          <input class="form-control" class="botao" type="submit" name="button" value="Encaminhar" />
        </dtml-if>
        <input class="form-control" class="botao" type="button" value="Voltar" onClick="location.href='&dtml-portal_url;/cadastros/tramitacao_documento/rascunhos_html'">
      </p>
     </form>
  </dtml-if>
<dtml-var standard_html_footer>
