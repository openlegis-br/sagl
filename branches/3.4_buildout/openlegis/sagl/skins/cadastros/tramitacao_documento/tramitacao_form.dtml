<script type="text/javascript">

(function ($) {
  $(function () {
    //$('input[type=submit]').hide();
    document.onkeypress = function (event) {
      var form = tramitacao_form;
      form.action = "tramitacao_salvar_proc";
      if (event.keyCode === 13) {
        campos_criticar(form);
      }
    };
  });
}(jQuery));
</script>

