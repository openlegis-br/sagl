<div id="barra-conteudo">
<ul>
  <li>
    <a 
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/anexada/anexada_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/anexada/anexada_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/anexada/anexada_form')">
       class="selecionado"
      </dtml-if>
    href="<dtml-var portal_url>/cadastros/materia/anexada/index_html?cod_materia=<dtml-var cod_materia>">Anexada</a>
  </li>
  <li>
  <dtml-in expr="zsql.materia_obter_zsql(cod_materia=cod_materia)">
    <a
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/autoria/autoria_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/autoria/autoria_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/autoria/autoria_form')">
       class="selecionado"
      </dtml-if> 
    href="<dtml-var portal_url>/cadastros/materia/autoria/index_html?cod_materia=<dtml-var cod_materia>&dat_apresentacao=<dtml-var dat_apresentacao missing null="">">Autoria</a>
   </dtml-in>
    </li>
    <li>
        <a
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/documento_acessorio/documento_acessorio_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/documento_acessorio/documento_acessorio_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/documento_acessorio/documento_acessorio_form')">
       class="selecionado"
      </dtml-if>    
        href="<dtml-var portal_url>/cadastros/materia/documento_acessorio/index_html?cod_materia=<dtml-var cod_materia>">Documento Acessório</a>
    </li>
    <li>
        <a
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/emenda/emenda_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/emenda/emenda_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/emenda/emenda_form')">
       class="selecionado"
      </dtml-if>    
        href="<dtml-var portal_url>/cadastros/materia/emenda/index_html?cod_materia=<dtml-var cod_materia>">
          Emenda
           <dtml-in expr="zsql.emenda_obter_zsql(cod_materia=cod_materia,ind_exlcuido=0)">
             <dtml-if sequence-start>
               [<dtml-var sequence-length>]
             </dtml-if sequence-start>
           </dtml-in>
        </a>
    </li>
    <li>
        <a
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/numeracao/numeracao_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/numeracao/numeracao_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/numeracao/numeracao_form')">
       class="selecionado"
      </dtml-if>    
        href="<dtml-var portal_url>/cadastros/materia/numeracao/index_html?cod_materia=<dtml-var cod_materia>">Numeração</a>
    </li>
    <li>
        <a
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/despacho_inicial/despacho_inicial_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/despacho_inicial/despacho_inicial_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/despacho_inicial/despacho_inicial_form')">
       class="selecionado"
      </dtml-if>    
        href="<dtml-var portal_url>/cadastros/materia/despacho_inicial/index_html?cod_materia=<dtml-var cod_materia>">Parecer Comissão</a>
    </li>
    <li>
        <a
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/substitutivo/substitutivo_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/substitutivo/substitutivo_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/substitutivo/substitutivo_form')">
       class="selecionado"
      </dtml-if>    
        href="<dtml-var portal_url>/cadastros/materia/substitutivo/index_html?cod_materia=<dtml-var cod_materia>">Substitutivo</a>
    </li>
    <li>
        <a 
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/tramitacao/tramitacao_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/tramitacao/tramitacao_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/tramitacao/tramitacao_form')">
       class="selecionado"
      </dtml-if>   
        href="<dtml-var portal_url>/cadastros/materia/tramitacao/index_html?cod_materia=<dtml-var cod_materia>">Tramitação</a>
    </li>
    <li>
        <a 
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/materia/votacao/votacao_index_html')">
       class="selecionado"
      </dtml-if>   
        href="<dtml-var portal_url>/cadastros/materia/votacao/index_html?cod_materia=<dtml-var cod_materia>">Votação</a>
    </li>
    <li>
        <a href="<dtml-var portal_url>/cadastros/materia/materia_mostrar_proc?cod_materia=<dtml-var cod_materia>">Início</a>
    </li>
</ul>
</div>
