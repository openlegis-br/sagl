<div id="barra-conteudo">
<ul>
  <dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Modulo Administrativo'])">
    <li>
      <a 
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/documento_acessorio/documento_acessorio_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/documento_acessorio/documento_acessorio_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/documento_acessorio/documento_acessorio_form')">
       class="selecionado"
      </dtml-if>
      href="&dtml-portal_url;/cadastros/documento_administrativo/documento_acessorio/index_html?cod_documento=<dtml-var cod_documento>">Documento Acessório</a>
    </li>
    <li>
      <a 
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/documento_vinculado/documento_vinculado_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/documento_vinculado/documento_vinculado_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/documento_vinculado/documento_vinculado_form')">
       class="selecionado"
      </dtml-if>
      href="&dtml-portal_url;/cadastros/documento_administrativo/documento_vinculado/index_html?cod_documento=<dtml-var cod_documento>">Documento Vinculado</a>
    </li>
  </dtml-if>
 <dtml-if expr="AUTHENTICATED_USER.has_role(['Operador', 'Operador Modulo Administrativo', 'Operador Materia'])">
    <li>
      <a 
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/materia_vinculada/materia_vinculada_index_html') or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/materia_vinculada/ementa_buscar_proc')">
       class="selecionado"
      </dtml-if>
      href="&dtml-portal_url;/cadastros/documento_administrativo/materia_vinculada/index_html?cod_documento=<dtml-var cod_documento>">Matéria Vinculada</a>
    </li>
    <li>
      <a
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/tramitacao/tramitacao_index_html')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/tramitacao/tramitacao_mostrar_proc')
      or _['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/tramitacao/tramitacao_form')">
       class="selecionado"
      </dtml-if>
       href="&dtml-portal_url;/cadastros/documento_administrativo/tramitacao/index_html?cod_documento=<dtml-var cod_documento>">Tramitação</a>
    </li>
    <li>
      <a
      <dtml-if expr="_['REQUEST']['ACTUAL_URL']==(portal_url() + '/cadastros/documento_administrativo/destinatario/destinatario_index_html')">
       class="selecionado"
      </dtml-if>
       href="&dtml-portal_url;/cadastros/documento_administrativo/destinatario/index_html?cod_documento=<dtml-var cod_documento>">Destinatários</a>
    </li>
 </dtml-if>

    <li>
        <a href="&dtml-portal_url;/cadastros/documento_administrativo/documento_administrativo_mostrar_proc?cod_documento=<dtml-var cod_documento>">Início</a>
    </li>
</ul>
</div>
