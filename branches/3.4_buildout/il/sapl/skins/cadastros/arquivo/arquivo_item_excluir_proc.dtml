<dtml-try>
  <dtml-call expr="zsql.arquivo_item_excluir_zsql(cod_item = cod_item)">
<dtml-except>
  <dtml-let mensagem="'Não foi possível excluir o item!'">
     <dtml-var mensagem_emitir>
  </dtml-let>
<dtml-else>
   <dtml-let mensagem="'Item excluído com sucesso!'" sucesso="1" url="'arquivo_recipiente_listar_conteudo_form?cod_recipiente='+cod_recipiente">
      <dtml-var mensagem_emitir>
   </dtml-let>
</dtml-try>
