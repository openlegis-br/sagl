<dtml-comment>
connection_id : dbcon_interlegis
arguments : cod_materia
max_rows : 1
max_cache : 100
</dtml-comment>
SELECT 
       MAX(cod_tramitacao) as cod_tramitacao
    
FROM   
     tramitacao 
       
WHERE
     cod_materia        = <dtml-sqlvar cod_materia type="int"> AND
     ind_ult_tramitacao = 0 AND
     ind_excluido       = 0
