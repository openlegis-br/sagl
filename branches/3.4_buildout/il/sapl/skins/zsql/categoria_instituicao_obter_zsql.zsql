<dtml-comment>
connection_id : dbcon_interlegis
arguments : tip_instituicao="" ind_excluido=0
max_rows : 1000
max_cache : 100
</dtml-comment>
SELECT
  cod_categoria,
  tip_instituicao,
  des_categoria,
  ind_excluido 
FROM 
  categoria_instituicao

<dtml-sqlgroup where>
  <dtml-sqltest tip_instituicao      op="=" type="int"  optional> <dtml-and>
  <dtml-sqltest ind_excluido         op="=" type="int"  optional>
</dtml-sqlgroup>

ORDER BY
  des_categoria
