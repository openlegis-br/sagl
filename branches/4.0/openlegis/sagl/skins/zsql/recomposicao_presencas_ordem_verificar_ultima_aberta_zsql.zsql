<dtml-comment>
connection_id : dbcon_openlegis
arguments : cod_sessao_plen
max_rows : 1000
max_cache : 100
</dtml-comment>

SELECT
  REPLACE (MAX(cod_registro_pre), 'L', '') as cod_registro_pre
FROM
  registro_presenca_ordem
WHERE
  cod_sessao_plen = <dtml-sqlvar cod_sessao_plen type="int">
  AND ind_status_pre = 1