<dtml-call expr="REQUEST.set('id_parecer', _.str(cod_relatoria)+'_parecer.pdf')">
<dtml-call expr="REQUEST.set('status','D')">
<dtml-if expr="_.hasattr(sapl_documentos.parecer_comissao,id_parecer)">
    <dtml-try>
	  <dtml-call expr="sapl_documentos.parecer_comissao.manage_delObjects(id_parecer)">
	  <dtml-except>
        <dtml-call expr="REQUEST.set('erro', 1)">
    </dtml-try>
</dtml-if>

<dtml-unless erro>
  <dtml-try>
  <dtml-call expr="zsql.relatoria_excluir_zsql(cod_relatoria=cod_relatoria)">
<dtml-except>  
  <dtml-call expr="REQUEST.set('erro', 1)"> 
<dtml-else>
  <dtml-let mensagem="'Relatoria excluÃ­da com sucesso!'">
     <dtml-var mensagem_emitir>
  </dtml-let>
  </dtml-try>
</dtml-unless>

<dtml-if erro>
  <dtml-let mensagem="'Ocorreu um erro ao tentar excluir a Relatoria!'" tipo_mensagem="'danger'">
     <dtml-var mensagem_emitir>
  </dtml-let>
</dtml-if>
