<dtml-call expr="REQUEST.set('erro', 0)">
<dtml-call expr="REQUEST.set('id_texto', _.str(cod_relatoria)+'_parecer.odt')">
<dtml-if expr="_.hasattr(sapl_documentos.parecer_comissao,id_texto)">
  <dtml-try>
    <dtml-call "sapl_documentos.parecer_comissao.manage_delObjects(id_texto)">
  <dtml-except>
    <dtml-call expr="REQUEST.set('erro', 1)">
  </dtml-try>
  
  <dtml-if erro>
    <dtml-let mensagem="'Ocorreu um erro ao tentar excluir o arquivo ODT do parecer!'" tipo_mensagem="'warning'">
      <dtml-var mensagem_emitir>
    </dtml-let>
  <dtml-else>
    <dtml-let mensagem="'Arquivo ODT excluÃ­do com sucesso!'" url="'relatoria_mostrar_proc?cod_relatoria='+cod_relatoria+'&modal=1'">
      <dtml-var mensagem_emitir>
    </dtml-let>
  </dtml-if>
</dtml-if>
