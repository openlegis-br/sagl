<dtml-var standard_html_header>

<a id="ajuda" href="<dtml-var portal_url>/ajuda/tramitacao_lote">Ajuda</a>

<h1 class="firstHeading">Tramitação em Lote</h1>

<script type="text/javascript" src="javascript/data_validar.js" "></script>

