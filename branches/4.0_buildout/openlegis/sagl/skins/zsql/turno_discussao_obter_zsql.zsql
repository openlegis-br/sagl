<dtml-comment>
connection_id : dbcon_interlegis
arguments : cod_turno sgl_turno
max_rows : 100
max_cache : 100
</dtml-comment>
SELECT
  cod_turno,
  cod_turno as cod_turno_sel,
  sgl_turno,
  des_turno

FROM 
  turno_discussao

<dtml-sqlgroup where>
  <dtml-sqltest cod_turno op="=" type="int" optional><dtml-and>
  <dtml-sqltest sgl_turno op="=" type="nb" optional><dtml-and>
  ind_excluido = 0
</dtml-sqlgroup>

ORDER BY
      des_turno
