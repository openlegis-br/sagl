<dtml-comment>
connection_id : dbcon_interlegis
arguments : cod_quorum
max_rows : 100
max_cache : 100
</dtml-comment>
SELECT 
   cod_quorum,
   des_quorum,
   txt_formula,
   ind_excluido 
FROM 
   quorum_votacao

<dtml-sqlgroup where>
  <dtml-sqltest cod_quorum op="=" type="int" optional><dtml-and>
  ind_excluido = 0
</dtml-sqlgroup>

ORDER BY
      cod_quorum
