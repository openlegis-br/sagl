<dtml-comment>
connection_id : dbcon_interlegis
arguments : cod_tramitacao cod_usuario_corrente
max_rows : 1
max_cache : 100
</dtml-comment>
UPDATE
    tramitacao
SET
    dat_recebimento = NOW(),
    cod_usuario_dest = <dtml-sqlvar cod_usuario_corrente type="int" optional>

<dtml-sqlgroup where>
   <dtml-sqltest cod_tramitacao op="=" type="int">
</dtml-sqlgroup>
