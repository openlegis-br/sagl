<dtml-comment>
connection_id : dbcon_interlegis
arguments : cod_materia
max_rows : 1000
max_cache : 100
</dtml-comment>
SELECT DISTINCT cod_materia

FROM tramitacao

<dtml-sqlgroup where>
  tramitacao.ind_excluido = 0

<dtml-and>
  <dtml-sqltest cod_materia column="tramitacao.cod_materia" op="=" type="int" optional>

</dtml-sqlgroup>
