    <dtml-sqlvar cod_materia     type="int">,
    <dtml-sqlvar dat_ordem       type="nb">,
    <dtml-sqlvar txt_observacao  type="string">,
    <dtml-sqlvar ind_excluido    type="int">,
    <dtml-sqlvar num_ordem       type="int">,
    <dtml-sqlvar tip_votacao     type="int">,
