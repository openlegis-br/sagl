      <dtml-in expr="zsql.usuario_unid_tram_obter_zsql(cod_usuario=cod_usuario_corrente)">
         <dtml-if expr="_.has_key('cod_ult_unidade') and int(cod_ult_unidade) == int(cod_unid_tramitacao)">
            <dtml-call expr="REQUEST.set('add_doc', 1)">
         </dtml-if>
      </dtml-in>
     <dtml-else>
      <dtml-call expr="REQUEST.set('metodo_acessorio', zsql.documento_acessorio_obter_zsql(cod_materia=cod_materia, ind_publico=1, ind_excluido=0))">
       <dtml-in metodo_acessorio>
          <dtml-call expr="REQUEST.set('ind_publico', 1)">       
