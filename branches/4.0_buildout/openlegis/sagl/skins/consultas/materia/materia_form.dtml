 </dtml-let>

 <dtml-if expr="_.has_key('temDocAC') or _.has_key('temDocADM') or _.has_key('add_doc')"> 
  <div id="accordion" class="documentos">
    <div class="card mb-4">
      <div class="card-header" id="headingOne">
        <p class="h5 mb-0">
          <a href="#" class="d-block text-left collapsed" data-toggle="collapse" data-target="#collapseDoc" aria-expanded="false" aria-controls="collapseDoc"> 
            <i class="fa float-right pt-1 align-self-center" aria-hidden="true"></i>
             <span class="h6 font-weight-bold">Documentos</span>
          </a>
        </p>
       </div>
      <div id="collapseDoc" class="collapse" aria-labelledby="headingOne" data-parent=".documentos">
      <div class="card-body">
         <dtml-let isAnon="portal_membership.isAnonymousUser()">
             <dtml-if expr="not isAnon and _.has_key('add_doc')">      
                 <button type="button" class="btn btn-sm btn-secondary" data-toggle="popover" title="Documento Acessório"><i class="fas fa-plus-circle"></i> Incluir</button>
                 <div id="PopoverContent" class="d-none">
                   <form name="myform" id="myform" class="needs-validation" validate role="form" enctype="multipart/form-data" method="post" action="documento_acessorio_salvar_proc" onSubmit="return confirm('Confirma a inclusão do documento acessório?');">
                    <input type="hidden" name="cod_materia" value="<dtml-var cod_materia>" />
                    <input type="hidden" name="cod_usuario" value="<dtml-var cod_usuario_corrente missing>" />
                    <input type="hidden" name="cod_unid_tram_local" value="<dtml-var cod_ult_unidade missing>" />
                    <div class="row">
                      <div class="col-12 mb-3">
                       <label for="lst_tip_documento" class="required">Tipo</label>
                         <select class="custom-select" id="lst_tip_documento" name="lst_tip_documento" required>
                             <option></option> 
                             <dtml-in expr="zsql.tipo_documento_obter_zsql(ind_excluido=0)">           
                                <option value="<dtml-var tip_documento>"> <dtml-var des_tipo_documento></option>
                             </dtml-in>                   
                          </select>
                          <div class="invalid-feedback">
                            Este campo é obrigatório!
                          </div>
                      </div>
                      <div class="col-12 mb-2">
                        <label for="txt_nom_documento" class="required">Descrição</label>        
                        <input class="form-control" type="text" id="txt_nom_documento" name="txt_nom_documento" value="" maxlength="50" required />
                          <small id="passwordHelpInline" class="text-muted">
                             Máximo 50 caracteres.
                          </small>          
                         <div class="invalid-feedback">
                           Este campo é obrigatório!
                         </div> 
                     </div>                    
                     <div class="col-12">                      
                      <div class="form-group">
                         <label for="file_nom_arquivo" class="required">Arquivo</label>
                         <input type="file" class="form-control-file" id="file_nom_arquivo" name="file_nom_arquivo" accept="application/pdf" required />
                      </div>
                     </div>
                     <div class="col-12">                      
                        <button class="btn btn-primary btn-block" type="submit" data-toggle="popover" data-placement="bottom" data-html="true" data-title="OK"><i class="fa fa-fw fa-check"></i> Incluir Documento</button>                     
                     </div>
                     <div class="col-md-12 mt-2">
                       <p class="text-muted small"><i class="fas fa-asterisk text-danger"></i> Campos obrigatórios</p>
                     </div>
                   </div>
                </form> 
              </div>   
            </dtml-if>
          </dtml-let>
          
         <dtml-in metodo_acessorio>
            <dtml-if sequence-start>
              <p class="text-center font-weight-bold h6">Documentos Acessórios</p>
              <div class="table-responsive">            
               <table class="table table-sm mb-0">
                 <thead">
                   <tr>
                     <th>Identificação do Documento</th>
                     <th>Autoria</th>                                          
                     <th>Data / Horário</th>
                   </tr>
                 </thead>
             </dtml-if>
                   <tr>
                     <td>
                       <dtml-call expr="REQUEST.set('id_documento', _.str(cod_documento)+'.pdf')">
                       <dtml-if expr="_.hasattr(sapl_documentos.materia,id_documento)">
                          <dtml-let documento="_.getattr(sapl_documentos.materia,id_documento).absolute_url">
                             <a href="<dtml-var documento>?<dtml-var expr="ZopeTime().timeTime()">" target="_blank">
                               <i class="fas fa-file-pdf"></i> <dtml-var des_tipo_documento> - <dtml-var nom_documento missing>
                             </a>
                           <dtml-if expr="sapl_documentos.props_sagl.restpki_access_token!='' and dbcon_logs">
                             <dtml-let isAnon="portal_membership.isAnonymousUser()">                      
                                <dtml-if expr="not isAnon">
                                  <dtml-in expr="zsql.logs_pesquisar_zsql(cod_registro=cod_documento, modulo='documento_acessorio_materia', ordem=1)">
                                    <dtml-call expr="REQUEST.set('usuario_doc', usuario)">
                                  </dtml-in>
                                 <dtml-in expr="zsql.usuario_obter_zsql(col_username=AUTHENTICATED_USER.getUserName())">
                                    <dtml-call expr="REQUEST.set('usuario_logado', col_username)">
                                 </dtml-in>
                                </dtml-if>
                              </dtml-let>
                              <dtml-if expr="_.has_key('add_doc') and _.has_key('usuario_doc') and _.has_key('usuario_logado') and usuario_doc==usuario_logado">
                                <dtml-in expr="zsql.assinatura_documento_obter_zsql(tipo_doc='doc_acessorio', codigo=cod_documento)">
                                  <dtml-call "REQUEST.set('cod_assinatura_doc', cod_assinatura_doc)">
                                </dtml-in>
                                <dtml-if expr="not _.has_key('cod_assinatura_doc')">
                                   <button type="button" class="btn btn-sm btn-secondary" data-toggle="modal" data-target="#iFrameModal" data-title="Assinatura Digital" data-src="<dtml-var portal_url>/generico/assinador/pades-signature_html?codigo=<dtml-var cod_documento>&tipo_doc=doc_acessorio&modal=1"><i class="fas fa-file-signature"></i> Assinar</button>
                                </dtml-if>
                                <button type="button" class="btn btn-sm btn-danger" onClick="documento_acessorio_excluir(<dtml-var cod_documento missing>)"><i class="fas fa-trash"></i> Excluir</button>          
                              </dtml-if>
                           </dtml-if>                             
                          </dtml-let>
                       <dtml-else>
                          <dtml-var des_tipo_documento> - <dtml-var nom_documento missing>
                       </dtml-if>
                     </td>
                     <td>
                        <dtml-if expr="nom_autor_documento!=None">
                           <dtml-var nom_autor_documento>
                        </dtml-if>
                      </td>                      
                     <td><dtml-var dat_documento></td>
                   </tr>
            <dtml-if sequence-end>
               </table>
           </dtml-if sequence-end>  
         </dtml-in>           

         <dtml-in expr="zsql.documento_administrativo_materia_obter_zsql(cod_materia=cod_materia,ind_excluido=0)">
          <dtml-in expr="zsql.documento_administrativo_obter_publico_zsql(cod_documento=cod_documento,ind_excluido=0)">
            <dtml-if sequence-start> 
              <p class="text-center font-weight-bold h6">Documentos Administrativos</p>
              <div class="table-responsive">            
               <table class="table table-sm mb-0">
                 <thead>
                  <tr>
                    <th>Identificação do Documento</th>
                    <th>Data</th>
                  </tr>
                 </thead>
            </dtml-if sequence-start>
            <dtml-in expr="zsql.documento_administrativo_obter_publico_zsql(cod_documento=cod_documento, ind_excluido=0)">
                <tr>
                  <td>
                    <dtml-call expr="REQUEST.set('id_documento', _.str(cod_documento)+'_texto_integral.pdf')">
                     <dtml-if "_.hasattr(sapl_documentos.administrativo,id_documento)">
                       <dtml-let documento="_.getattr(sapl_documentos.administrativo,id_documento).absolute_url">
                         <a target="_blank" href="<dtml-var documento>">
                           <b><dtml-var des_tipo_documento> nº <dtml-var num_documento>/<dtml-var ano_documento></b>
                         </a>
                       </dtml-let>
                     <dtml-else>
                        <a href="<dtml-var portal_url>/consultas/documento_administrativo/documento_administrativo_mostrar_proc?cod_documento=<dtml-var cod_documento>">
                           <b><dtml-var des_tipo_documento> nº <dtml-var num_documento>/<dtml-var ano_documento></b>
                        </a>
                     </dtml-if>
                     - <dtml-var txt_interessado> - <dtml-var txt_assunto>
                  </td>
                  <td><dtml-var dat_documento></td>
                </tr>
            </dtml-in>
            <dtml-if sequence-end>
               </table>
             </div>
           </dtml-if sequence-end>  
         </dtml-in> 
       </dtml-in>   
     </div>
   </div>
 </div>
 </div>
 </dtml-if>


<dtml-if expr="zsql.tramitacao_obter_zsql(cod_materia=cod_materia)">
